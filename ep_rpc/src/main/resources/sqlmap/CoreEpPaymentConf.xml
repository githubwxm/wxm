<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.all580.ep.dao.CoreEpPaymentConfMapper">
    <!--<resultMap id="BaseResultMap" type="com.all580.ep.entity.Ep">-->
        <!--<id column="id" property="id" jdbcType="INTEGER"/>-->
        <!--<id column="name" property="name" jdbcType="VARCHAR"/>-->
        <!--<result column="en_name" property="en_name" jdbcType="VARCHAR"/>-->
    <!--</resultMap>-->
    <select id="findById" resultType="com.all580.ep.api.entity.CoreEpPaymentConf">
       select *  from t_core_ep_payment_conf
        where
        1=1
        <if test="id != null">
            and core_ep_id=#{core_ep_id}
        </if>
    </select>

    <insert id="create">
        insert into t_core_ep_payment_conf(core_ep_id ,payment_type,conf_data)
        values(#{core_ep_id} ,#{payment_type},#{conf_data});
    </insert>
</mapper>