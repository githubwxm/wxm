<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 远程rpc类声明 -->
	<!-- 提供方应用信息，用于计算依赖关系 -->
	<dubbo:application name="order-provider" />

	<!---->
	<!-- 使用zookeeper广播注册中心暴露服务地址 -->
	<dubbo:registry address="${dubbo.registry.zk}" />
	<dubbo:protocol name="dubbo" port="${dubbo.port}" />

	<bean id="epService" class="com.all580.ep.service.EpServiceImple" />

	<dubbo:service interface="com.all580.ep.api.service.EpService" ref="epService" />

	<bean id="coreEpAccessService" class="com.all580.ep.service.CoreEpAccessServiceImple" />

	<dubbo:service interface="com.all580.ep.api.service.CoreEpAccessService" ref="coreEpAccessService" />

	<bean id="coreEpChannelServiceImple" class="com.all580.ep.service.CoreEpChannelServiceImple" />

	<dubbo:service interface="com.all580.ep.api.service.CoreEpChannelService" ref="coreEpChannelServiceImple" />

	<bean id="epBalanceThresholdServiceImple" class="com.all580.ep.service.EpBalanceThresholdServiceImple" />

	<dubbo:service interface="com.all580.ep.api.service.EpBalanceThresholdService" ref="epBalanceThresholdServiceImple" />

	<bean id="logCreditServiceImple" class="com.all580.ep.service.LogCreditServiceImple" />

	<dubbo:service interface="com.all580.ep.api.service.LogCreditService" ref="logCreditServiceImple" />
	
</beans>